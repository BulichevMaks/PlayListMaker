<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <size
                android:width="312dp"
                android:height="312dp"
                android:gravity="center"/>

            <solid android:color="#1A1B22" />
            <stroke
                android:width="1dp"
                android:color="#AEAFB4"
                android:dashWidth="35dp"
                android:dashGap="35dp" />

            <corners android:radius="8dp" />
        </shape>

    </item>

    <item
        android:drawable="@drawable/add_new_playlist_placeholder"
        android:height="79dp"
        android:width="79dp"
        android:gravity="center" />
</layer-list>